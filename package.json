{
  "name": "ezcharts",
  "version": "0.1.0",
  "description": "Minimalist React Visualization Library",
  "author": "",
  "license": "MIT",
  "keywords": [
    "react",
    "charts",
    "visualization",
    "svg",
    "data-viz"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "pnpm build && pnpm typecheck"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vitest": "^1.3.1"
  }
}
